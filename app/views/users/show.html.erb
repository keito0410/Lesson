<%= render 'users/list', book: @book%>
<div class="col-xs-9">
<table class="table table-hover table-inverse">
	<thead>
		<tr>
			<th>Title</th>
			<th>Option</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<% current_user.books.each do |current_user_book| %>
			<td><%= current_user_book.title %></td>
			<td><%= current_user_book.body %></td>
			<% end %>
		</tr>
	</tbody>
</table>
    </div>
  </div>
</div> 